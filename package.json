{
  "name": "apex-log-utilities",
  "displayName": "Apex Log Utilities",
  "publisher": "GilGourevitch",
  "description": "Eases the Apex log retrieval by streaming Apex Logs from the default project org in a Terminal.",
  "icon": "images/logo.png",
  "version": "1.0.2",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "apex-log-utilities.openLogStream",
      "title": "Apex Log Utils: Open Log Stream"
    },{
      "command": "apex-log-utilities.openLogStreamInTab",
      "title": "Apex Log Utils: Open Log Stream in tab"
    },{
      "command": "apex-log-utilities.openDebugStream",
      "title": "Apex Log Utils: Open Debug Stream"
    },{
      "command": "apex-log-utilities.openDebugStreamInTab",
      "title": "Apex Log Utils: Open Debug Stream in tab"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gilgourevitch/vscext-apex-log-utilities.git"
  },
  "license": "MIT",
  "private": true
}
